{
  "name": "react-native-portal",
  "version": "1.0.2",
  "description": "Translocate your render destination, using mitt. Built with react@16 and react-native in mind.",
  "main": "index.js",
  "author": "Zenyr <zenyr@zenyr.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyr/react-native-portal.git"
  },
  "scripts": {
    "build": "yarn build:es5&&yarn build:min",
    "build:es5": "babel index.js --out-file index.es5.js --source-maps --presets=es2015 --plugins=transform-flow-strip-types,transform-class-properties",
    "build:min": "babel index.js --out-file index.min.js --source-maps --presets=babili,es2015 --plugins=transform-flow-strip-types,transform-class-properties"
  },
  "keywords": [
    "portal",
    "gateway",
    "react",
    "react-native"
  ],
  "bugs": {
    "url": "https://github.com/zenyr/react-native-portal/issues"
  },
  "homepage": "https://github.com/zenyr/react-native-portal",
  "license": "MIT",
  "dependencies": {
    "mitt": "*"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
