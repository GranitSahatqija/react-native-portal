{
  "name": "react-native-portal",
  "version": "1.0.2",
  "description": "Translocate your render destination, using mitt. Built with react@16 and react-native in mind.",
  "main": "index.commonjs.js",
  "module": "index.noflow.js",
  "author": "Zenyr <zenyr@zenyr.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyr/react-native-portal.git"
  },
  "scripts": {
    "noflow": "babel --no-babelrc index.js --out-file index.test.js --plugins=transform-flow-strip-types,transform-class-properties",
    "build": "rollup -c&&yarn noflow"
  },
  "keywords": [
    "portal",
    "gateway",
    "react",
    "react-native"
  ],
  "bugs": {
    "url": "https://github.com/zenyr/react-native-portal/issues"
  },
  "homepage": "https://github.com/zenyr/react-native-portal",
  "license": "MIT",
  "dependencies": {
    "mitt": "*"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
